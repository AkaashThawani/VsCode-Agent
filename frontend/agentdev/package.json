{
  "name": "agentdev",
  "displayName": "AgentDev",
  "description": "An autonomous AI developer agent.",
  "version": "0.0.1",
  "publisher": "akaashthawani",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "agentdev-sidebar-container",
          "title": "AgentDev",
          "icon": "media/robot.svg"
        }
      ]
    },
    "views": {
      "agentdev-sidebar-container": [
        {
          "id": "agentdev.statusView",
          "name": "AI Coding Assistant",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "agentdev.startTask.internal",
        "title": "AgentDev: Start Task (Internal)"
      },
      {
        "command": "agentdev.stopAgent.internal",
        "title": "AgentDev: Stop Agent (Internal)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.13",
    "node-fetch": "^2.7.0"
  }
}